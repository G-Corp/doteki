	, {modules, []}
	, {registered, []}
	, {applications, [kernel,stdlib]}

  , {maintainers, ["Gregoire Lejeune"]}
  , {licenses, ["BSD-3"]}
  , {links, [{"Github", "https://github.com/botsunit/doteki"}]}
